# Crawler Worker Environment Configuration

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/ai_seo_tool
DB_HOST=localhost
DB_PORT=5432
DB_NAME=ai_seo_tool
DB_USER=postgres
DB_PASSWORD=postgres

# Worker Configuration
POLL_INTERVAL_MS=5000
MAX_CONCURRENT_JOBS=2
WORKER_ID=worker-1

# Crawler Configuration
DEFAULT_MAX_PAGES=100
DEFAULT_MAX_DEPTH=3
DEFAULT_TIMEOUT=30000
STORE_RAW_HTML=true

# Core Web Vitals Configuration
ENABLE_CWV=true
CWV_TIMEOUT=60000
CWV_RETRIES=2

# Logging
LOG_LEVEL=info
