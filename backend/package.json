    "db:migrate:down": "node-pg-migrate down"
  },
  "keywords": ["seo", "ai", "orchestrator", "fastify"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "fastify": "^4.25.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "pg": "^8.11.3",
    "bullmq": "^5.1.0",
    "ioredis": "^5.3.2",
    "uuid": "^9.0.1",
    "pino": "^8.17.0",
    "pino-pretty": "^10.3.0",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.0",
    "tsx": "^4.6.0",
    "vitest": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "node-pg-migrate": "^6.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
